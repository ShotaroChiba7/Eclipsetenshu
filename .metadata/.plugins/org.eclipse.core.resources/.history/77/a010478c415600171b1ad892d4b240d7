package com.mycompany;

public class Sosu {

	boolean checksosu(int a){
		int i;
		int j = 0;
		boolean h = true;
		i = 1;
			do{
				if(a % i ==0){
					if (a == i){
						j = 2;//素数
						h = true;
					}else{
						j = 1;//not素数
						h = false;
					}
				}else{
					i = i + 1;
				}

			}while(j < 1);
				return h;
	}

}
