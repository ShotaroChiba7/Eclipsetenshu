package com.mycompany;

public class Sosu {

	boolean checksosu(int a){
		int i = 2;
		int j = 0;
		boolean h = true;

			do{
				System.out.println(a);
				System.out.println(i);
				System.out.println(a % i);
				if((a % i) == 0){
					if (a == i){
						j = 2;//素数
						h = true;
					}else{
						j = 13;//not素数
						h = false;
					}
				}else{
					i = i + 1;
				}
				System.out.println(i + " " +j);
			}while(j == 0);

				return h;
	}

}
